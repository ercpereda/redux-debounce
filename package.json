{
  "name": "redux-debounce",
  "version": "1.0.1",
  "description": "FSA-compliant middleware for Redux to debounce actions.",
  "license": "MIT",
  "repository": "wyze/redux-debounce",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "neilkistner.com"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf .nyc_output coverage lib",
    "coverage": "nyc report -r text-lcov | codecov",
    "lint": "eslint src test",
    "prebuild": "npm run lint",
    "pretest": "npm run clean && npm run build",
    "test": "nyc ava",
    "test:watch": "npm test -- --watch"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "wyze/base"
  },
  "files": [
    "lib",
    "license",
    "package.json",
    "readme.md"
  ],
  "keywords": [
    "redux",
    "debounce",
    "middleware",
    "redux-middleware",
    "fsa",
    "flux"
  ],
  "dependencies": {
    "flux-standard-action": "^0.6.1",
    "lodash.debounce": "^4.0.7",
    "lodash.mapvalues": "^4.5.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "codecov.io": "^0.1.6",
    "eslint": "^2.8.0",
    "eslint-config-wyze": "^1.3.0",
    "nyc": "^6.4.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3"
  }
}
